				<ProtectedString name="Source"><![CDATA[local ChangeHistoryService = game:GetService("ChangeHistoryService")
local StudioService = game:GetService("StudioService")
local Selection = game:GetService("Selection")
local template = script.Parent.MockShell
local scripts = {}
local p = "DeScript:"
type Array<T> = {[number]: T}

local function applyClassToIcon(image :ImageButton|ImageLabel, className :string)
	for prop,value in pairs(StudioService:GetClassIcon(className)) do 
		image[prop] = value
	end
end

local openNclose = plugin:CreateToolbar("DeScript"):CreateButton(math.random(),"","rbxasset://textures/StudioSharedUI/scripts.png","Open/Close")
openNclose.ClickableWhenViewportHidden = true

local window = plugin:CreateDockWidgetPluginGui("DeScriptDrWhoInTARDIS", DockWidgetPluginGuiInfo.new(Enum.InitialDockState.Float,true,false,350,300,200,100))
window.Title = "DeScript"
local gui = template:Clone()
gui.Size = UDim2.new(1,0,1,0)
gui.Position = UDim2.new()
gui.Parent = window

--Hook button to open/close
openNclose:SetActive(window.Enabled)
window:GetPropertyChangedSignal("Enabled"):Connect(function()
	openNclose:SetActive(window.Enabled)
end)
openNclose.Click:Connect(function()
	window.Enabled = not window.Enabled
end)

local loadedScripts :{[LuaSourceContainer]: Array<RBXScriptConnection>} = {}

local function clearLoadedScripts() 
	for _,child in pairs(gui.ScrollArea:GetChildren()) do
		if child:IsA("Frame") then child:Destroy() end
	end
	for aScript, cons in pairs(loadedScripts) do
		table.foreach(cons,function(_,con) con:Disconnect() end)
		loadedScripts[aScript] = nil
	end
end

--reset the gui and scan items in
local function loadScripts(area :Array<Instance>)
	--reset gui
	gui.ScrollArea.CanvasPosition = Vector2.new()
	clearLoadedScripts()
	
	--scan scripts in
	for _,subarea in pairs(area) do 
		for _,des :LuaSourceContainer in pairs(subarea:GetDescendants()) do
			if des:IsA("LuaSourceContainer") then 
				local row = template.ScrollArea.ScriptFrame:Clone()
				row.TextButton.Text = des:GetFullName()
				applyClassToIcon(row.Icon, des.ClassName)
				row.TextButton.Activated:Connect(function()
					Selection:Set({des})
				end)
				local nameCon,parentCon
				local function delRow() 
					nameCon:Disconnect()
					parentCon:Disconnect()
					row:Destroy()
					loadedScripts[des] = nil
				end
				nameCon = des:GetPropertyChangedSignal("Name"):Connect(function()
					row.TextButton.Text =des:GetFullName()
				end)
				parentCon = des:GetPropertyChangedSignal("Parent"):Connect(function()
					if not des.Parent 
					then delRow()
					else row.TextButton.Text = des:GetFullName()
					end
				end)
				row.Delete.Activated:Connect(function()
					ChangeHistoryService:SetWaypoint(p.." Deleting Script")
					des:Remove()
					ChangeHistoryService:SetWaypoint(p.." Deleting Script")
				end)
				
				loadedScripts[des] = {nameCon,parentCon}
				row.Parent = gui.ScrollArea
			end
		end
	end
end

gui.Delete.Activated:Connect(function()
	ChangeHistoryService:SetWaypoint(p.." Mass Deleting Scripts")
	for aScript, cons in pairs(loadedScripts) do
		aScript:Remove() --the parent connection handles the cleanup of the row
	end
	ChangeHistoryService:SetWaypoint(p.." Mass Deleted Scripts")
end)

gui.NonList.Clear.Activated:Connect(clearLoadedScripts)

gui.Search.Activated:Connect(function()
	loadScripts(Selection:Get())
	gui.Search.Text = "Found: "..#gui.ScrollArea:GetChildren()-1 .. " scripts!"
	delay(2.75,function()
		gui.Search.Text = template.Search.Text
	end)
end)


loadScripts({})


--setup and hook help
local help = "DeScript v2.2\nMade by DrWhoInTARDIS"
help ..= "\n\nHow To Use:"
help ..= "\n-- Select as many things you want to scan in the Explorer or Game Window."
help ..= "\n-- Click Search in the DeScript window"
help ..= "\n-- A list of scripts will appear in the window if found."
help ..= "\n-- The Icon shows the type of script"
help ..= "\n-- The text shows the path to the script, you can click it to goto the script."
help ..= "\n-- Clicking the trash can deletes the script."
help ..= "\n-- Clicking Delete All deletes all the scripts in the window."
help ..= "\n-- Clicking Clear clears the Gui and doesn't delete the scripts."
help ..= "\n\nHappy Hunting!"

local helpWindow = plugin:CreateDockWidgetPluginGui("DeScriptHelp",DockWidgetPluginGuiInfo.new(Enum.InitialDockState.Float,false,true,450,260))
helpWindow.Title = "DeScript Help"
script.Parent.Help.Info.Text = help
script.Parent.Help.Info:Clone().Parent = helpWindow
gui.NonList.Help.Activated:Connect(function()
	helpWindow.Enabled = not helpWindow.Enabled
end)


--[[
	Change Log:
	
	2.1 > 2.2: Added Clear Button and Waypoints
	2.0 > 2.1: Added Help
	1.0 > 2.0: Complete ReWrite. Added Gui
]]]]></ProtectedString>
